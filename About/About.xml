<?xml version="1.0" encoding="utf-8"?>

<ModMetaData>
	<name>Gas Vents</name>

	<packageId>NachoToast.GasVents</packageId>

	<author>NachoToast</author>

	<modVersion>2.0</modVersion>

	<url>https://github.com/NachoToast/RimWorldGasVents</url>

	<description>Create gas vents that can be loaded with gases and slowly disperse them over time when powered. Custom gases supported.</description>

	<supportedVersions>
		<li>1.4</li>
		<li>1.5</li>
	</supportedVersions>

	<modDependenciesByVersion>
		<v1.4>
			<li>
				<packageId>NachoToast.SCGF</packageId>
				<displayName>Simple Custom Gas Framework</displayName>
				<steamWorkshopUrl>steam://url/CommunityFilePage/2999444522</steamWorkshopUrl>
				<downloadUrl>https://github.com/NachoToast/SimpleCustomGasFramework/releases/latest</downloadUrl>
			</li>
		</v1.4>
		<v1.5>
			<li>
				<packageId>bs.performance</packageId>
				<displayName>Performance Fish</displayName>
				<downloadUrl>https://github.com/bbradson/performance-fish</downloadUrl>
			</li>
		</v1.5>
	</modDependenciesByVersion>

	<loadAfter>
		<li>bs.performance</li>
		<li>Ludeon.RimWorld</li>
		<li>Ludeon.RimWorld.Royalty</li>
		<li>Ludeon.RimWorld.Ideology</li>
		<li>Ludeon.RimWorld.Biotech</li>
		<li>Ludeon.RimWorld.Anomaly</li>
	</loadAfter>

	<incompatibleWith>
		<li>NachoToast.GasVents.VanillaOnlyEdition</li>
	</incompatibleWith>
</ModMetaData>